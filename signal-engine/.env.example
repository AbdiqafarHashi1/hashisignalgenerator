MARKET_DATA_REPLAY_PATH=data/replay
CANDLE_INTERVAL=5m
TICK_INTERVAL_SECONDS=60

# Account / prop constraints
ACCOUNT_SIZE=25000
PROP_ENABLED=true
PROP_PROFIT_TARGET_PCT=0.08
PROP_MIN_TRADING_DAYS=5
PROP_MAX_DAYS=60
PROP_MAX_DAILY_LOSS_PCT=0.05
PROP_MAX_GLOBAL_DD_PCT=0.10
PROP_GOVERNOR_ENABLED=true
PROP_RISK_BASE_PCT=0.0015
PROP_RISK_MAX_PCT=0.0020
PROP_RISK_MIN_PCT=0.0007
PROP_DAILY_STOP_AFTER_NET_R=2.0
PROP_DAILY_STOP_AFTER_LOSSES=2
PROP_MAX_TRADES_PER_DAY=4
PROP_MAX_CONSEC_LOSSES=2
PROP_TIME_COOLDOWN_MINUTES=60

# STRATEGY (PROP_PASS recommended defaults)
STRATEGY_BIAS_EMA_FAST=50
STRATEGY_BIAS_EMA_SLOW=200
STRATEGY_SWING_LOOKBACK=30
STRATEGY_ADX_THRESHOLD=22
STRATEGY_MIN_ATR_PCT=0.0015
STRATEGY_MAX_ATR_PCT=0.0105
STRATEGY_TREND_SLOPE_THRESHOLD=0.0002
STRATEGY_ATR_STOP_MULT=1.6
STRATEGY_SWING_STOP_BUFFER_BPS=8
STRATEGY_MIN_STOP_PCT=0.002
STRATEGY_MAX_STOP_PCT=0.015
STRATEGY_TARGET_R=2.2
STRATEGY_PARTIAL_R=1.8
DISABLE_BREAKOUT_CHASE=true
HTF_BIAS_ENABLED=true
HTF_INTERVAL=1h
REQUIRE_CANDLE_CLOSE_CONFIRM=true

# Management
BE_ENABLED=true
BE_TRIGGER_R=1.3
BE_BUFFER_BPS=1.2
BE_MIN_SECONDS_OPEN=300
PARTIAL_TP_ENABLED=true
PARTIAL_TP_R=1.8
PARTIAL_TP_CLOSE_PCT=0.25
TRAIL_ENABLED=true
TRAIL_START_R=2.2
TRAIL_ATR_MULT=1.2

# Optional integrations
TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

DATA_DIR=data
DATABASE_URL=sqlite:///data/trades.db
